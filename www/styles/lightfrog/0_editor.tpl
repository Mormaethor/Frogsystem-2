<!--section-start::BUTTON--><td class="editor_button">
  <div {..javascript..}>
    <img src="$VAR(style_icons)/editor/{..img_file_name..}" alt="{..alt..}" title="{..title..}">
  </div>
</td><!--section-end::BUTTON-->

<!--section-start::SEPERATOR--><td class="editor_seperator"></td><!--section-end::SEPERATOR-->

<!--section-start::SMILIES_BODY--><td class="editor_smilies center top">
  <b class="small">Smilies</b>
  {..smilies_table..}
</td>


<!--section-end::SMILIES_BODY-->

<!--section-start::BODY--><div class="editor_button_line left">
  <table cellpadding="0" cellspacing="0">
    <tr class="bottom">
      {..buttons..}
    </tr>
  </table>
</div>

<div class="editor left">
  <table cellpadding="0" cellspacing="0">
    <tr>
      <td>
        <textarea rows="5" cols="50" name="{..name..}" id="{..id..}">{..text..}</textarea>
      </td>
     
    </tr>
  </table>
</div>
<!--section-end::BODY-->

